select * from APP.PORTFOLIOTD;

drop table app.pf;
drop table app.pf_td;

create table APP.pf (id int not null primary key, DSC varchar(10));
create table APP.pf_td (pf_id int not null, startdate date not null, naam varchar(10));

alter table app.pf_td add  primary key(pf_id, startdate);

--alter table APP.pf add primary key(id);
alter table APP.pf_td add foreign key(pf_id)  references APP.pf(id);